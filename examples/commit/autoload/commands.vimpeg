.skip_white = true
.namespace = 'commands_parser'
.parser_name = 'commands#parser'
.root_element = 'commands'

commands ::= show | set
set      ::= 'set' (sym (('=' . (val | sym)) | '!')?)? -> #set
show     ::= 'show' sym?                     -> #show
sym      ::= '\h\w*'
val      ::= '\d\+'
